<Window x:Class="Labb03_QuizApplication.Dialogs.ImportStatusDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labb03_QuizApplication.Dialogs"
        mc:Ignorable="d"
        Title="ImportStatusDialog" Height="200" Width="300">
    <Grid>
        <TextBlock Text="{Binding StatusReport}"
               FontSize="20"
               VerticalAlignment="Center"
               HorizontalAlignment="Center"
               TextWrapping="Wrap"/> 
    </Grid>
</Window>
